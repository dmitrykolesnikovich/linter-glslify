{
    "name": "linter-glslify",
    "description": "Linter for GLSL shaders, powered with glslify",
    "version": "1.0.0",
    "author": "fand <fand.gmork@gmail.com> (https://gmork.in/)",
    "bugs": "https://github.com/fand/linter-glslify/issues",
    "dependencies": {
        "atom-message-panel": "1.3.0",
        "atom-package-deps": "5.1.0",
        "convert-source-map": "^1.6.0",
        "execa": "^1.0.0",
        "glslang-validator-prebuilt": "0.0.2",
        "glslify-lite": "0.0.7",
        "source-map": "^0.8.0-beta.0",
        "temp-write": "^4.0.0",
        "which": "1.3.1"
    },
    "devDependencies": {
        "@types/atom": "^1.36.1",
        "@types/convert-source-map": "^1.5.1",
        "@types/execa": "^0.9.0",
        "@types/node": "^12.6.8",
        "@types/source-map": "^0.5.7",
        "@types/which": "^1.3.1",
        "@typescript-eslint/eslint-plugin": "^1.12.0",
        "@typescript-eslint/parser": "^1.12.0",
        "eslint": "^6.0.1",
        "eslint-config-prettier": "^6.0.0",
        "eslint-plugin-jest": "^22.10.0",
        "eslint-plugin-prettier": "^3.1.0",
        "glsl-noise": "^0.0.0",
        "husky": "^3.0.0",
        "jasmine-fix": "1.3.1",
        "lint-staged": "^9.2.0",
        "prettier": "^1.18.2",
        "standard-version": "^8.0.1",
        "typescript": "^3.5.3"
    },
    "engines": {
        "atom": ">=1.0.0 <2.0.0"
    },
    "homepage": "https://github.com/fand/linter-glslify",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "keywords": [
        "Atom",
        "GLSL",
        "Lint",
        "glslify",
        "sourcemaps"
    ],
    "license": "MIT",
    "lint-staged": {
        "*.{js,ts,json,md}": [
            "prettier --write",
            "git add"
        ]
    },
    "main": "./lib/linter-glslify",
    "package-deps": [
        "linter:2.0.0"
    ],
    "providedServices": {
        "linter": {
            "versions": {
                "2.0.0": "provideLinter"
            }
        }
    },
    "repository": "https://github.com/fand/linter-glslify",
    "scripts": {
        "build": "tsc -d",
        "lint": "eslint src/**/*.ts",
        "test": "apm test"
    }
}
